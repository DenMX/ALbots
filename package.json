{
  "type": "module",
  "name": "albots",
  "version": "1.0.0",
  "description": "Adventure Land TypeScript bots",
  "main": "src/main.ts",
  "scripts": {
    "dev": "concurrently \"tsx src/main.ts\" \"cd src/vue-monitor && npm run dev\"",
    "build": "tsc",
    "build-vue": "node scripts/simple-build-vue.js",
    "build-cursor-ui": "cd src/cursor-ui && npm install && npm run build",
    "start": "tsx src/main.ts",
    "obs": "npx tsx src/GlobalObserver.ts",
    "clean": "rm -rf dist node_modules"
  },
  "author": "DenMX",
  "license": "MIT",
  "dependencies": {
    "alclient": "^0.24.1",
    "bot-web-interface": "^2.0.14",
    "dependencies": "^0.0.1",
    "express": "^5.2.1",
    "fastpriorityqueue": "^0.7.5",
    "pretty-ms": "^9.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^25.0.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "keywords": []
}
